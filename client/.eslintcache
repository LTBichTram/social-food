patch = useDispatch();\n\n  const [saved, setSaved] = useState(false);\n  const [saveLoad, setSaveLoad] = useState(false);\n\n  // Likes\n  useEffect(() => {\n    if (post.likes.find((like) => like._id === auth.user._id)) {\n      setIsLike(true);\n    } else {\n      setIsLike(false);\n    }\n  }, [post.likes, auth.user._id]);\n\n  const handleLike = async () => {\n    if (loadLike) return;\n\n    setLoadLike(true);\n    await dispatch(likePost({ post, auth, socket }));\n    setLoadLike(false);\n  };\n\n  const handleUnLike = async () => {\n    if (loadLike) return;\n\n    setLoadLike(true);\n    await dispatch(unLikePost({ post, auth, socket }));\n    setLoadLike(false);\n  };\n\n  // Saved\n  useEffect(() => {\n    if (auth.user.saved.find((id) => id === post._id)) {\n      setSaved(true);\n    } else {\n      setSaved(false);\n    }\n  }, [auth.user.saved, post._id]);\n\n  const handleSavePost = async () => {\n    if (saveLoad) return;\n\n    setSaveLoad(true);\n    await dispatch(savePost({ post, auth }));\n    setSaveLoad(false);\n  };\n\n  const handleUnSavePost = async () => {\n    if (saveLoad) return;\n\n    setSaveLoad(true);\n    await dispatch(unSavePost({ post, auth }));\n    setSaveLoad(false);\n  };\n\n  return (\n    <div className=\"card_footer\">\n      <div className=\"card_icon_menu\">\n        <div>\n          <LikeButton\n            isLike={isLike}\n            handleLike={handleLike}\n            handleUnLike={handleUnLike}\n          />\n\n          <Link to={`/post/${post._id}`} className=\"text-dark\">\n            <i className=\"far fa-comment\" />\n          </Link>\n\n          <i\n            class=\"fa-regular fa-paper-plane\"\n            onClick={() => setIsShare(!isShare)}\n          ></i>\n        </div>\n\n        {saved ? (\n          <i className=\"fas fa-bookmark text-info\" onClick={handleUnSavePost} />\n        ) : (\n          <i className=\"far fa-bookmark\" onClick={handleSavePost} />\n        )}\n      </div>\n\n      <div className=\"d-flex justify-content-between\">\n        <h6\n          style={{\n            padding: \"0 25px\",\n            cursor: \"pointer\",\n            color: \"rgb(226 215 255)\",\n          }}\n        >\n          {post.likes.length} likes\n        </h6>\n\n        <h6\n          style={{\n            padding: \"0 25px\",\n            cursor: \"pointer\",\n            color: \"rgb(226 215 255)\",\n          }}\n        >\n          {post.comments.length} comments\n        </h6>\n      </div>\n\n      {isShare && (\n        <ShareModal url={`${BASE_URL}/post/${post._id}`} theme={theme} />\n      )}\n    </div>\n  );\n};\n\nexport default CardFooter;\n","D:\\Workspace\\social-food\\client\\src\\components\\message\\Times.js",[],"D:\\Workspace\\social-food\\client\\src\\redux\\actions\\commentAction.js",[],"D:\\Workspace\\social-food\\client\\src\\utils\\config.js",[],"D:\\Workspace\\social-food\\client\\src\\components\\Carousel.js",[],"D:\\Workspace\\social-food\\client\\src\\components\\LikeButton.js",[],"D:\\Workspace\\social-food\\client\\src\\components\\ShareModal.js",[],"D:\\Workspace\\social-food\\client\\src\\components\\home\\comments\\CommentDisplay.js",[],"D:\\Workspace\\social-food\\client\\src\\components\\home\\comments\\CommentCard.js",["387"],"D:\\Workspace\\social-food\\client\\src\\components\\home\\comments\\CommentMenu.js",[],"D:\\Workspace\\social-food\\client\\src\\components\\alert\\Alert.jsx",[],"D:\\Workspace\\social-food\\client\\src\\components\\alert\\Loading.jsx",[],"D:\\Workspace\\social-food\\client\\src\\components\\alert\\Toast.jsx",[],{"ruleId":"388","replacedBy":"389"},{"ruleId":"390","replacedBy":"391"},{"ruleId":"392","severity":1,"message":"393","line":1,"column":21,"nodeType":"394","messageId":"395","endLine":1,"endColumn":29},{"ruleId":"396","severity":1,"message":"397","line":40,"column":11,"nodeType":"398","endLine":40,"endColumn":59},{"ruleId":"396","severity":1,"message":"397","line":24,"column":15,"nodeType"